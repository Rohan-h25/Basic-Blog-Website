<%- include("partials/header") -%>

<h1>Home</h1>
<p> <%= homeText %> </p>

<% homePost.forEach(function(element) { %>

  <h1> <%= element.title %> </h1>

  <% const body = element.body.slice(0,100) + " ..."; %>

   <%# const title = "/posts/" + element.title.replaceAll(' ','');  %>

  <p>
    <%= body %>
    <a href= "/posts/<%= element.title %>" >Read More</a>
  </p>

<% }); %>

<%- include("partials/footer") -%>
